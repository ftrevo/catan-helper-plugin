{
  "manifest_version": 3,
  "name": "My First Chrome Extension",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "permissions": ["activeTab", "clipboardWrite", "clipboardRead"],
  "background": {
    "service_worker": "static/js/background.js"
  }
}
